{
  "name": "webpack-study",
  "version": "0.0.1",
  "description": "webpack单页应用",
  "main": "src/index.js",
  "scripts": {
    "builda": "cross-env WEBPACK_ANALYZER=1 NODE_ENV=production webpack --config build/webpack.prod.js --colors --progress",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js --colors --progress",
    "pre": "cross-env NODE_ENV=preproduction webpack --config build/webpack.prod.js --colors --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --colors --progress --config build/webpack.dev.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js src",
    "fix": "eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my-webpack-practice/webpack-study.git"
  },
  "keywords": [
    "webpack-study"
  ],
  "author": "lizhigao <lizhigao1231@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/my-webpack-practice/webpack-study/issues"
  },
  "homepage": "https://github.com/my-webpack-practice/webpack-study#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.5",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob-all": "^3.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-notifier": "^5.3.0",
    "node-sass": "^4.9.4",
    "postcss": "^7.0.5",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.1.1",
    "prettier": "1.14.3",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "zepto-webpack": "^1.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {},
      "cssnano": {}
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 version",
    "iOS >= 8",
    "Android >= 4.4"
  ]
}
